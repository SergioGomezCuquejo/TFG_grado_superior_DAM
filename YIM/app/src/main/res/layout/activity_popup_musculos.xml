<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/_style_rectangulo_fondo_clarito__borde_blanco"
    android:gravity="center"
    android:orientation="vertical"
    tools:context=".vista.vista.PopupMusculos">


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingLeft="20dp"
        android:paddingRight="20dp"
        android:paddingTop="20dp"
        android:gravity="center"
        android:orientation="horizontal">

        <ImageView
            android:id="@+id/cancelar"
            android:layout_width="47dp"
            android:layout_height="47dp"
            android:contentDescription="@string/imagen_cancelar"
            tools:srcCompat="@drawable/cancelar" />

        <Space
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_weight="1" />

        <ImageView
            android:id="@+id/guardar"
            android:layout_width="42dp"
            android:layout_height="42dp"
            tools:srcCompat="@drawable/guardar"
            android:contentDescription="@string/imagen_guardar" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:background="@drawable/_style_rectangulo_fondo_clarito__borde_negro"
        android:baselineAligned="false"
        android:gravity="center"
        android:orientation="vertical"
        tools:ignore="UselessParent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/_style_rectangulo_fondo_clarito__borde_negro"
            android:baselineAligned="false"
            android:orientation="horizontal"
            android:paddingLeft="20dp"
            android:paddingRight="20dp"
            android:paddingBottom="20dp"
            tools:ignore="UselessParent">

            <FrameLayout
                android:layout_width="0dp"
                android:layout_height="70dp"
                android:layout_weight="1"
                android:background="@drawable/_style_rectangulo_gris_oscuro__borde_blanco_10"
                android:padding="1dp"
                tools:ignore="NestedWeights">

                <EditText
                    android:id="@+id/musculo"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:background="@drawable/_style_rectangulo_gris_oscuro__borde_blanco_10"
                    android:fontFamily="@font/merriweather_light"
                    android:gravity="center"
                    android:text="@string/biceps"
                    android:textColor="@color/negro_oscuro"
                    android:textSize="24sp"
                    android:textStyle="bold"
                    tools:ignore="LabelFor,NestedWeights"
                    android:autofillHints="Musculo"
                    android:inputType="text" />
            </FrameLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="20dp"
            android:gravity="center_horizontal|center_vertical"
            android:orientation="horizontal">

            <View
                android:layout_width="0dp"
                android:layout_height="3dp"
                android:layout_marginStart="20dp"
                android:layout_weight="1"
                android:background="@drawable/_style_rectangulo_gris" />

            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:layout_weight="2"
                android:fontFamily="@font/merriweather_light"
                android:gravity="center"
                android:text="@string/cambiar_colores"
                android:textColor="@color/negro_clarito"
                android:textSize="20sp"
                android:textStyle="bold" />

            <View
                android:layout_width="0dp"
                android:layout_height="3dp"
                android:layout_marginEnd="20dp"
                android:layout_weight="1"
                android:background="@drawable/_style_rectangulo_gris" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/colorFondo"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="10dp"
            android:layout_marginRight="20dp"
            android:background="@drawable/_style_rectangulo_fondo_clarito__borde_negro"
            android:baselineAligned="false"
            android:orientation="horizontal"
            tools:ignore="UselessParent">

            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="2"
                android:fontFamily="@font/merriweather_light"
                android:text="@string/color_del_fondo"
                android:textColor="@color/negro_clarito"
                android:textSize="20sp"
                tools:ignore="NestedWeights" />

            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:fontFamily="@font/merriweather_light"
                android:gravity="right"
                android:text="@string/_88888"
                android:textColor="@color/negro_clarito"
                android:textSize="20sp"
                android:textStyle="bold"
                tools:ignore="RtlHardcoded" />

        </LinearLayout>

        <LinearLayout
            android:id="@+id/colorLetras"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="20dp"
            android:layout_marginRight="20dp"
            android:background="@drawable/_style_rectangulo_fondo_clarito__borde_negro"
            android:baselineAligned="false"
            android:orientation="horizontal"
            tools:ignore="UselessParent">

            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="2"
                android:fontFamily="@font/merriweather_light"
                android:text="@string/color_de_las_letras"
                android:textColor="@color/negro_clarito"
                android:textSize="20sp"
                tools:ignore="NestedWeights" />

            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:fontFamily="@font/merriweather_light"
                android:gravity="right"
                android:text="@string/_88888"
                android:textColor="@color/negro_clarito"
                android:textSize="20sp"
                android:textStyle="bold"
                tools:ignore="RtlHardcoded" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="40dp"
            android:gravity="center_horizontal|center_vertical"
            android:orientation="horizontal">

            <View
                android:layout_width="0dp"
                android:layout_height="3dp"
                android:layout_marginStart="20dp"
                android:layout_weight="1"
                android:background="@drawable/_style_rectangulo_gris" />

            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:layout_weight="2"
                android:fontFamily="@font/merriweather_light"
                android:gravity="center"
                android:text="@string/informacion_adicional"
                android:textColor="@color/negro_clarito"
                android:textSize="20sp"
                android:textStyle="bold" />

            <View
                android:layout_width="0dp"
                android:layout_height="3dp"
                android:layout_marginEnd="20dp"
                android:layout_weight="1"
                android:background="@drawable/_style_rectangulo_gris" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="10dp"
            android:layout_marginRight="20dp"
            android:background="@drawable/_style_rectangulo_fondo_clarito__borde_negro"
            android:baselineAligned="false"
            android:orientation="horizontal"
            tools:ignore="UselessParent">

            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="3"
                android:fontFamily="@font/merriweather_light"
                android:text="@string/usos_esta_semana"
                android:textColor="@color/negro_clarito"
                android:textSize="20sp"
                tools:ignore="NestedWeights" />

            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:fontFamily="@font/merriweather_light"
                android:gravity="right"
                android:text="@string/_888"
                android:textColor="@color/negro_clarito"
                android:textSize="20sp"
                android:textStyle="bold"
                tools:ignore="RtlHardcoded" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="20dp"
            android:layout_marginRight="20dp"
            android:background="@drawable/_style_rectangulo_fondo_clarito__borde_negro"
            android:baselineAligned="false"
            android:orientation="horizontal"
            tools:ignore="UselessParent">

            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="3"
                android:fontFamily="@font/merriweather_light"
                android:text="Ejercicios realizados:"
                android:textColor="@color/negro_clarito"
                android:textSize="20sp"
                tools:ignore="NestedWeights" />

            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:fontFamily="@font/merriweather_light"
                android:gravity="right"
                android:text="@string/_888"
                android:textColor="@color/negro_clarito"
                android:textSize="20sp"
                android:textStyle="bold"
                tools:ignore="RtlHardcoded" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="20dp"
            android:layout_marginRight="20dp"
            android:background="@drawable/_style_rectangulo_fondo_clarito__borde_negro"
            android:baselineAligned="false"
            android:orientation="horizontal"
            tools:ignore="UselessParent">

            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="3"
                android:fontFamily="@font/merriweather_light"
                android:text="@string/ejercicios_sin_realizar"
                android:textColor="@color/negro_clarito"
                android:textSize="20sp"
                tools:ignore="NestedWeights" />

            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:fontFamily="@font/merriweather_light"
                android:gravity="right"
                android:text="@string/_888"
                android:textColor="@color/negro_clarito"
                android:textSize="20sp"
                android:textStyle="bold"
                tools:ignore="RtlHardcoded" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="20dp"
            android:layout_marginRight="20dp"
            android:background="@drawable/_style_rectangulo_fondo_clarito__borde_negro"
            android:baselineAligned="false"
            android:orientation="horizontal"
            tools:ignore="UselessParent">

            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="3"
                android:fontFamily="@font/merriweather_light"
                android:text="@string/ejercicios_totales"
                android:textColor="@color/negro_clarito"
                android:textSize="20sp"
                tools:ignore="NestedWeights" />

            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:fontFamily="@font/merriweather_light"
                android:gravity="right"
                android:text="@string/_888"
                android:textColor="@color/negro_clarito"
                android:textSize="20sp"
                android:textStyle="bold"
                tools:ignore="RtlHardcoded" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="20dp"
            android:layout_marginRight="20dp"
            android:background="@drawable/_style_rectangulo_fondo_clarito__borde_negro"
            android:baselineAligned="false"
            android:orientation="horizontal"
            tools:ignore="UselessParent">

            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="3"
                android:fontFamily="@font/merriweather_light"
                android:text="@string/usado_en_rutinas"
                android:textColor="@color/negro_clarito"
                android:textSize="20sp"
                tools:ignore="NestedWeights" />

            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:fontFamily="@font/merriweather_light"
                android:gravity="right"
                android:text="@string/_888"
                android:textColor="@color/negro_clarito"
                android:textSize="20sp"
                android:textStyle="bold"
                tools:ignore="RtlHardcoded" />
        </LinearLayout>


    </LinearLayout>


</LinearLayout>